--- DATABASE SCHEMAS ---

--- BEGIN USER SESSION SCHEMA (prototype)

CREATE TABLE UserSession (
    id TEXT NOT NULL PRIMARY KEY,
    fbId TEXT NOT NULL,
    avatar TEXT NOT NULL,
    name TEXT
);

insertUserSession: INSERT INTO UserSession(id, fbId, avatar, name) VALUES(?, ?, ?, ?);

removeUserSession: DELETE FROM UserSession;

selectUserSession: SELECT * FROM UserSession;

--- END USER SCHEMA